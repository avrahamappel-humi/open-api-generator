{
    "name": "humi/open-api-generator",
    "description": "Generate OpenAPI specs from our defined routes",
    "authors": [
        {
            "name": "Avraham Appel",
            "email": "avraham.appel@humi.ca"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^8.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/yaml": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.24",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5",
        "glhd/laravel-dumper": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Humi\\OpenApiGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["Humi\\OpenApiGenerator\\ServiceProvider"]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
